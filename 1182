l = int(input())
s = str(input())
matriz = []
soma = 0
for i in range(12):
    h = []
    for j in range(12):
        n = float(input())
        h.append(n)
    matriz.append(h)
for k in range(12):
    soma+= matriz[k][l]
if s == 'S':
    print('{:.1f}'.format(soma))
if s == 'M':
    print('{:.1f}'.format(soma/12))
