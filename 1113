xy = input().split()
x = int(xy[0])
y = int(xy[1])
if x>y:
    print("Decrescente")
elif y>x:
    print('Crescente')
else:
    print("")
while (x>y or y>x):
    xy = input().split()
    x = int(xy[0])
    y = int(xy[1])
    if x>y:
        print("Decrescente")
    elif y>x:
        print('Crescente')
